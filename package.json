    "cors": "^2.8.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.0",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.13.16",
    "react": "^18.2.0",
    "react-redux": "^8.0.5",
